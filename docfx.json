{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "VKProxy",
      "_appLogoPath": "https://fs7744.github.io/VKProxy.Doc/images/logo.jpg",
      "_appFaviconPath": "https://fs7744.github.io/VKProxy.Doc/images/favicon.ico",
      "_appTitle": "VKProxy",
      "_appFooter": "<a href=\"https://github.com/fs7744/VKProxy\">VKProxy</a> Â© Victor.X.Qu",
      "_enableSearch": true,
      "pdf": false
    }
  }
}